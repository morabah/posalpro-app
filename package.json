{
  "name": "posalpro-mvp2",
  "version": "0.2.1-alpha.3",
  "private": true,
  "engines": {
    "node": ">=20.17.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "dev:clean": "npm run dev:smart",
    "dev:smart": "./scripts/dev-clean.sh",
    "dev:demo": "./scripts/demo-port-management.sh",
    "kill:dev": "pkill -f 'next dev' || true",
    "build": "next build",
    "analyze": "ANALYZE=true next build",
    "prebuild": "npm run rebuild-native",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "test:accessibility": "jest --testPathPattern=accessibility",
    "test:performance": "node scripts/test-optimizations.js",
    "test:real-world": "node scripts/run-real-world-tests.js",
    "test:comprehensive": "npm run test:critical-gaps && npm run test:api-routes && npm run test:integration && npm run test:real-world",
    "test:critical-gaps": "jest src/test/critical-gaps --coverage --verbose",
    "test:api-routes": "jest src/test/api-routes --coverage --verbose",
    "test:integration": "jest src/test/integration --coverage --verbose",
    "test:mobile": "jest src/test/critical-gaps/mobile-touch-interactions.test.tsx --coverage --verbose",
    "test:type-safety": "jest src/test/api-routes/type-safety-validation.test.ts --coverage --verbose",
    "test:database-validation": "jest src/test/critical-gaps/database-agnostic-validation.test.ts --coverage --verbose",
    "test:security": "jest --testNamePattern=\"Security|security\" --coverage --verbose",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "quality:check": "npm run lint && npm run type-check && npm run format:check",
    "prepare": "npx husky",
    "pre-commit": "npm run quality:check && npm run test:ci",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --name",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset --force",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:format": "prisma format",
    "db:validate": "prisma validate",
    "db:reset": "prisma migrate reset --force && npm run db:seed",
    "postinstall": "prisma generate",
    "production:setup": "./scripts/setup-production.sh",
    "production:validate": "./scripts/setup-production.sh validate",
    "production:seed": "./scripts/setup-production.sh seed-only",
    "production:backup": "./scripts/setup-production.sh backup-mocks",
    "rebuild-native": "npm rebuild bcrypt --build-from-source",
    "version:alpha": "npm version prerelease --preid=alpha",
    "version:beta": "npm version prerelease --preid=beta",
    "version:rc": "npm version prerelease --preid=rc",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "deploy": "./scripts/deploy.sh",
    "deploy:alpha": "./scripts/deploy.sh alpha",
    "deploy:beta": "./scripts/deploy.sh beta",
    "deploy:rc": "./scripts/deploy.sh rc",
    "deploy:patch": "./scripts/deploy.sh patch",
    "deploy:minor": "./scripts/deploy.sh minor",
    "deploy:major": "./scripts/deploy.sh major",
    "deploy:staging": "./scripts/deploy.sh staging",
    "deploy:dry-run": "./scripts/deploy.sh alpha --dry-run",
    "version:update-history": "node scripts/update-version-history.js",
    "predeploy": "npm run version:update-history",
    "predeploy:alpha": "npm run version:update-history",
    "predeploy:beta": "npm run version:update-history",
    "predeploy:rc": "npm run version:update-history",
    "deployment:info": "node scripts/deployment-info.js",
    "audit:duplicates": "node scripts/audit-duplicates.js",
    "cache:clear": "rm -rf .next && rm -rf node_modules/.cache",
    "fix:zod": "npm run cache:clear && npm install --legacy-peer-deps && npm run dev:smart",
    "install:safe": "npm install --legacy-peer-deps",
    "test:authenticated": "node scripts/test-proposals-authenticated.js",
    "performance:monitor": "node scripts/performance-monitor.js",
    "memory:optimization": "node scripts/memory-optimization-test.js",
    "ci:obs": "node scripts/check-observability-contract.js",
    "ci:bundle": "ANALYZE=true next build && node scripts/check-bundle-budgets.js",
    "app:cli": "tsx scripts/app-cli.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,mdx,json,css,scss}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --config eslint.config.mjs"
    ]
